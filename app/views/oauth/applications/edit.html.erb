<h1>Edit</h1>

<%= form_for @application, url: oauth_application_path(@application) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br />

  <%= f.label :redirect_uri, "Callback URL" %>
  <%= f.url_field :redirect_uri %><br />

  <%= link_to "Cancel", :back %>
  <%= f.submit "Update Application" %>
<% end %>
