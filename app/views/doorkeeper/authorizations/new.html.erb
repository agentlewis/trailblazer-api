<tb-oauth-authorization
  googleApiKey="<%= ENV['GOOGLE_BROWSER_API_KEY'] %>"
  userName="<%= current_user.name %>"
  userEmail="<%= current_user.email %>"
  userUid="<%= current_user.uid %>"
  canSignIn="<%= current_user.domain_id.present? ? true : false %>"
  formTarget="<%= oauth_authorization_url %>"
  clientId="<%= @pre_auth.client.uid %>"
  clientName="<%= @pre_auth.client.name %>"
  redirectURI="<%= @pre_auth.redirect_uri %>"
  state="<%= @pre_auth.state %>"
  responseType="<%= @pre_auth.response_type %>"
  scope="<%= @pre_auth.scope %>"
  changeURL="<%= change_user_url(return_to: request.url) %>"></tb-oauth-authorization>
